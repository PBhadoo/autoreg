#!/usr/bin/perl
#
# $Id$
#

# local configuration.
require "/usr/local/autoreg/conf/config";
require "$DNSLIB/val.pl";

$date=`date +%Y%m%d`; chop $date;

my $rq = $ARGV[0];
my $user = $ARGV[1];

if ($rq && $rq !~ /[a-zA-Z0-9-_]/) {
  print "Bad filename: $rq\n";
  exit;
}

&doaccept($rq, $user, $date);
