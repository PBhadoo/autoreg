query = SELECT email
          FROM contacts, handle_secrets
          WHERE handle = substring (upper('%u') FROM '^([A-Z]+[0-9]+)-')
            AND MD5(handle_secrets.secret || handle || '-' || id::text) =
                substring (lower('%u') from '-([0-9a-f]+)$')
domain = handles.eu.org
user = postfix
hosts = unix:
expansion_limit = 1
